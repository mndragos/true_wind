#:kivy 1.11.1

<GraphicSolution>:
    size_hint: (None, None)
    size: (300, 380)
    pos_hint: {'x': .2, 'top':1}
    title_align: 'center'
    title_size: template_font_size
    title_color: 0,0,0,1
    text_size: self.size
    title: "GRAPHIC SOLUTION"
    separator_color: 1, 1, 1, 0
    background: "true_wind\\img\\snow_white"
    s_speed: 20
    BoxLayout:
        orientation:'vertical'
        TemplateLabel:
            background_color: 1, 1, 1,1
            line_color: .72,.52,.04, 0
        TemplateButton:
            text: 'CLOSE'
            background_normal: "true_wind\\img\\antique_white.png"
            background_down: "true_wind\\img\\fire_brick.png"
            pos_hint: {'x': .35, 'y':.2}
            size_hint: (.3, .1)
            on_press: root.dismiss()
